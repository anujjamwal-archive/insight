{
  "compilerOptions": {
    "target": "es5",
    "outDir": "dist",
    "rootDir": ".",
    "jsx": "react",
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "module": "esnext",
    "strict": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2017", "dom"]
  }
}
